{
    "name": "Instagram Live Captions",
    "version": "0.1",
    "manifest_version": 3,
    "description": "インスタライブに字幕をつける拡張機能",
    "content_scripts": [{
        "matches": ["https://www.instagram.com/*/live/"],
        "js": ["content.js"]
    }],
    "icons": {
        "16": "icon/icon16.png",
        "48": "icon/icon48.png",
        "128": "icon/icon128.png"
    }
}
